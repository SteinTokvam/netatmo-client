  "license": "MIT",
  "author": {
    "name": "SteinTokvam",
    "url": "https://github.com/SteinTokvam"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SteinTokvam/netatmo-client.git"
  },
  "bugs": {
    "url": "https://github.com/SteinTokvam/netatmo-client/issues"
  },
  "keywords": [
    "Netatmo",
    "Weather"
  ],
  "homepage": "https://github.com/SteinTokvam/netatmo-client",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
