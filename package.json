  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "@eslint/js": "8.56.0",
    "eslint": "8.56.0",
    "globals": "^15.8.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts"
}